{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "app",
            "image": "023004991146.dkr.ecr.ap-south-1.amazonaws.com/tides:latest",
            "memory": 128,
            "command": [
                "tail",
                "-f",
                "/bin/bash"
            ],
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 4000
                }
            ],
            "links": [
                "build"
            ]
        },
        {
            "name": "build",
            "image": "alpine:3.11",
            "memory": 128,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 4000
                }
            ]
        }
    ]
}