{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "app",
            "image": "023004991146.dkr.ecr.ap-south-1.amazonaws.com/tides:latest",
            "memory": 128,
            "command": [
                "tail",
                "-f",
                "/bin/bash"
            ],
            "portMappings": [
                {
                    "hostPort": 4000,
                    "containerPort": 4000
                }
            ],
            "links": [
                "db"
            ]
        },
        {
            "name": "db",
            "image": "postgres:10-alpine",
            "memory": 128,
            "command": [
                "tail",
                "-f",
                "/bin/bash"
            ],
            "portMappings": [
                {
                    "hostPort": 5432,
                    "containerPort": 5432
                }
            ]
        }
    ]
}